{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/server.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "functions": {
    "backend/server.cjs": {
      "runtime": "nodejs18.x"
    }
  },
  "ignoreCommand": false
}

